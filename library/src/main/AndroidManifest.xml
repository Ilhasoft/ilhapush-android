<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="br.com.ilhasoft.push">

    <uses-sdk tools:overrideLibrary="br.com.ilhasoft.flowrunner" />

    <application
        android:allowBackup="true"
        android:label="@string/ilhapush_app_name"
        android:supportsRtl="true">

        <activity android:name=".chat.IlhaPushChatActivity"/>

        <service
            android:name=".services.RegistrationIntentService"
            android:exported="false"/>

        <service android:name=".services.InstanceIdIntentService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.gms.iid.InstanceID"/>
            </intent-filter>
        </service>

    </application>

</manifest>
